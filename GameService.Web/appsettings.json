{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AdminSettings": {
    "ApiKey": ""
  },
  "AllowedHosts": "*",
  "GameService": {
    "Economy": {
      "InitialCoins": 100
    },
    "Security": {
      "MinimumApiKeyLength": 32,
      "EnforceApiKeyValidation": true
    },
    "Database": {
      "MaxPoolSize": 50,
      "MinPoolSize": 5,
      "ConnectionIdleLifetime": 300,
      "ConnectionTimeout": 30,
      "CommandTimeout": 30,
      "Pooling": true
    }
  }
}
